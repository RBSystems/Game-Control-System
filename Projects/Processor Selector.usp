#SYMBOL_NAME "Processor Selector"
#CATEGORY "32" 

DIGITAL_INPUT _SKIP_;
DIGITAL_INPUT _SKIP_;
DIGITAL_INPUT _SKIP_;
DIGITAL_INPUT _SKIP_;
DIGITAL_INPUT Select;
DIGITAL_INPUT ResetProcessor;
INTEGER_PARAMETER Processor01Input;
INTEGER_PARAMETER Processor02Input;
INTEGER_PARAMETER Processor01Output;
INTEGER_PARAMETER Processor02Output;

DIGITAL_OUTPUT _SKIP_;
DIGITAL_OUTPUT _SKIP_;
DIGITAL_OUTPUT _SKIP_;
DIGITAL_OUTPUT _SKIP_;
DIGITAL_OUTPUT Processor01Active;
DIGITAL_OUTPUT Processor02Active;
ANALOG_OUTPUT SelectedInput;
ANALOG_OUTPUT SelectedOutput;

INTEGER Switched;

PUSH Select
{
	if(Switched = 1)
	{
		SelectedOutput = Processor02Output;
		SelectedInput = Processor02Input;
		Processor02Active = 1;
		Processor01Active = 0;
		Switched = 0;
	}
	else
	{
		SelectedOutput = Processor01Output;
		SelectedInput = Processor01Input;
		Processor02Active = 0;
		Processor01Active = 1;
		Switched = 1;
	}
}

PUSH ResetProcessor
{
	Switched=0;
	SelectedOutput = 0;
	SelectedInput = 0;
	Processor02Active = 0;
	Processor01Active = 0;
}
